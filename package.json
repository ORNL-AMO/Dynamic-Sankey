{
  "name": "easysankey",
  "version": "0.0.1",
  "description": "Dynamic sankey creater desktop application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager .  --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Easy Sankey App\"",
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Michael Whitmer",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "d3": "4.12.2 ",
    "electron": "^1.7.9",
    "jquery": "^3.2.1",
    "material-design-icons": "^3.0.1",
    "spectrum-colorpicker": "^1.8.0"
  },
  "devDependencies": {
    "electron-packager": "^9.1.0"
  }
}
